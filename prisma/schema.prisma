generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Culture {
  id          Int      @id @default(autoincrement())
  key         String   @unique
  name        String
  region      String
  population  String
  language    String
  location    String
  coordinates Int[]
  description String  
  traditions  String
  lifestyle   String
  images      String[] @db.Text
}
